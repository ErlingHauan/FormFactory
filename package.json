{
  "dependencies": {
    "@digdir/design-system-react": "^0.50.0",
    "@digdir/design-system-tokens": "^0.13.0",
    "@navikt/aksel-icons": "^5.17.1",
    "i18next": "^23.9.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@swc/core": "^1.4.8",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/css-modules": "^1.0.5",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "packageManager": "yarn@4.0.2",
  "prettier": "./config/prettier.config.js",
  "private": true,
  "scripts": {
    "build": "yarn workspaces foreach -A -p run build",
    "format": "prettier frontend --write && dotnet format backend/FormAPI.sln",
    "format-backend": "dotnet format backend/FormAPI.sln",
    "format-frontend": "prettier frontend --write",
    "lint": "yarn workspaces foreach -A -p run lint",
    "preflight": "yarn lint && yarn format && yarn test",
    "preflight-backend": "yarn format-backend && yarn test-backend",
    "preflight-frontend": "yarn lint && yarn format-frontend && yarn test-frontend",
    "reseed-db": "docker-compose down db && docker volume rm formfactory_db && docker-compose up -d --no-deps --build db && yarn seed-db",
    "seed-db": "dotnet ef database update --project backend/src/FormAPI.csproj",
    "setup-db": "docker-compose up -d --no-deps --build db && yarn seed-db",
    "setup-docker": "docker-compose up --build -d && yarn seed-db",
    "start-backend": "dotnet run --project backend/src/FormAPI.csproj",
    "start-frontend": "yarn workspace main-app run start",
    "test": "jest --config=./frontend/test/jest.config.js && dotnet test backend/FormAPI.sln",
    "test-backend": "dotnet test backend/FormAPI.sln",
    "test-frontend": "jest --config=./frontend/test/jest.config.js",
    "typecheck": "yarn workspaces foreach -A -p run typecheck"
  },
  "workspaces": [
    "frontend/form-builder",
    "frontend/main-app"
  ]
}
